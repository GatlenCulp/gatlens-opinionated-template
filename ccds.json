{
	"project_name": "project_name",
	"repo_name": "{{ cookiecutter.project_name.lower().replace(' ', '_') }}",
	"module_name": "{{ cookiecutter.project_name.lower().replace(' ', '_').replace('-', '_') }}",
	"author_name": "Your name, organization, or team",
	"description": "A short description of the project.",
	"python_version_number": "3.12",
	"dataset_storage": [
		{ "none": "none" },
		{ "azure": { "container": "container-name" } },
		{ "s3": { "bucket": "bucket-name", "aws_profile": "default" } },
		{ "gcs": { "bucket": "bucket-name" } }
	],
	"environment_manager": ["uv", "none", "virtualenv", "conda", "pipenv"],
	"dependency_file": ["requirements.txt", "environment.yml", "Pipfile"],
	"pydata_packages": ["basic", "none"],
	"open_source_license": ["MIT", "No license file", "BSD-3-Clause"],
	"docs": ["mkdocs", "none"],
	"include_code_scaffold": [
		"data",
		"No",
		"paper",
		"app",
		"ml",
		"lib",
		"course"
	],
	"version_control": [
		"git (local)",
		"none",
		"git (github private)",
		"git (github public)"
	],
	"_github_username": "GatlenCulp",
	"_use_conventional_commits": "y",
	"_generate_personal_ssh_keys": "n",
	"_generate_and_upload_gh_deploy_keys": "n",
	"_project_emoji": "üç™",
	"_readme_modern_header": "y",
	"_readme_include_logo": "y",
	"_readme_include_screenshots": "y",
	"_readme_use_github_discussions": "n",
	"_readme_include_badges": "y",
	"_readme_include_toc": "n",
	"_readme_include_project_assistance": "y",
	"_readme_include_authors": "y",
	"_readme_include_security": "y",
	"_readme_include_acknowledgements": "y",
	"_readme_table_in_about": "y",
	"_copy_without_render": [
		"*{{ cookiecutter.module_name }}/_backend/**",
		"*{{ cookiecutter.module_name }}/_frontend/**",
		"*{{ cookiecutter.module_name }}/_cli/**"
	]
}
